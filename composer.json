{
    "name": "how/howwp",
    "description": "How WP Theme packages",
    "authors": [
        {
            "name": "How",
            "email": "lowhow@gmail.com"
        }
    ],
    "repositories": [
    		{
		      "type": "composer",
		      "url": "http://wpackagist.org"
		    },
		    {
		      "type": "package",
		      "package": {
		        "name": "wordpress",
		        "type": "webroot",
		        "version": "4.4",
		        "dist": {
		          "type": "zip",
		          "url": "https://github.com/WordPress/WordPress/archive/4.4.zip"
		        },
		        "require": {
		          "fancyguy/webroot-installer": "1.0.0"
		        }
		      }
		    }
    ],
    "require":
    {
		    "wordpress": "4.4",
		    "fancyguy/webroot-installer": "1.0.0",
		    "wpackagist-plugin/akismet": "dev-trunk",
		    "wpackagist-plugin/regenerate-thumbnails": ">=2.2.4",
		    "wpackagist-plugin/wp-pagenavi": ">=2.8",
		    "wpackagist-plugin/acf-field-date-time-picker": ">=2",
		    "wpackagist-plugin/members": ">=1.0.2",
		    "wpackagist-plugin/wp-mail-smtp": ">=0.9.5",
		    "wpackagist-plugin/wordpress-importer": ">=0.6.1",
		    "wpackagist-plugin/responsive-lightbox": ">=1.6.6",
		    "wpackagist-plugin/advanced-cron-manager": ">=1.4.3",
		    "wpackagist-plugin/members": ">=1.1.0",
		    "wpackagist-plugin/woocommerce": ">=2.4.12"
    },
    "extra":
    {
		    "webroot-dir": "cms",
		    "webroot-package": "wordpress",
		    "installer-paths":
		    {
		    	"content/plugins/{$name}": ["type:wordpress-plugin"]
		    }

	},
	"autoload":
	{
			"psr-4":
			{
				"Framework\\": "content/themes/blect/framework"
			}
	}
}
